{
    "name": "mediumart/orange",
    "type": "library",
    "license": "MIT",
    "keywords": ["orange", "orange-api", "sms", "laravel"],
    "description": "A laravel wrapper/adapter for orange api services.",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
     "require": {
        "php": ">= 5.6",
        "mediumart/orange-sms": "~1.1",
        "illuminate/support": "~5.1|6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7|~6.0|~7.0|^8.3",
        "mockery/mockery": "0.9.*|~1.0",
        "orchestra/testbench": "~3.1|4.*"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Orange\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediumart\\Orange\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mediumart\\Orange\\OrangeServiceProvider"
            ]
        }
    }
}
