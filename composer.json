{
    "name": "mediumart/orange",
    "type": "library",
    "license": "MIT",
    "keywords": ["orange", "orange-api", "sms", "laravel"],
    "description": "A laravel adapter for orange api services.",
    "authors": [
        {
            "name": "Isaac Esso",
            "email": "daprod2009@gmail.com"
        }
    ],
     "require": {
        "php": ">= 5.6",
        "mediumart/orange-sms": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Mediumart\\Orange\\Laravel\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orange\\Laravel\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "mediumart/notifier": "Allows you to use custom Aliases/hooks names for custom notifications channels"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mediumart\\Orange\\Laravel\\OrangeServiceProvider"
            ]
        }
    }
}
